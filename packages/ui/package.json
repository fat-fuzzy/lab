{
	"package": "@fat-fuzzy/ui",
	"name": "@fat-fuzzy/ui",
	"version": "0.0.1",
	"type": "module",
	"main": "./src/lib/index.ts",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"sb": "start-storybook -p 6006",
		"build:sb": "build-storybook"
	},
	"author": "Patricia Boh",
	"license": "Apache-2.0",
	"exports": {
		".": {
			"import": "./src/lib/"
		},
		"./lib/*": [
			"./src/lib/**/*.svelte",
			"./src/lib/**/*.ts",
			"./src/lib/**/*.js",
			"./src/lib/**/*.scss"
		],
		"./data/*": [
			"./src/data/**/*.ts",
			"./src/data/**/*.js"
		]
	},
	"devDependencies": {
		"@babel/core": "7.17.8",
		"@playwright/test": "^1.20.0",
		"@storybook/addon-actions": "6.5.13",
		"@storybook/addon-essentials": "6.5.13",
		"@storybook/addon-links": "6.5.13",
		"@storybook/addon-svelte-csf": "2.0.10",
		"@storybook/addons": "6.5.13",
		"@storybook/api": "6.5.13",
		"@storybook/client-api": "6.5.13",
		"@storybook/client-logger": "6.5.13",
		"@storybook/components": "6.5.13",
		"@storybook/core-events": "6.5.13",
		"@storybook/preset-scss": "1.0.3",
		"@storybook/svelte": "6.5.13",
		"@storybook/theming": "6.5.13",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"@xstate/svelte": "0.2.1",
		"babel-loader": "8.2.3",
		"css-loader": "6.7.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-storybook": "0.5.11",
		"eslint-plugin-svelte3": "^3.2.1",
		"jest": "27.5.1",
		"postcss": "8.4.12",
		"postcss-load-config": "4.0.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"sass": "1.49.9",
		"sass-loader": "12.6.0",
		"style-loader": "3.3.1",
		"svelte": "^3.53.1",
		"svelte-check": "^2.7.1",
		"svelte-loader": "3.1.2",
		"svelte-preprocess": "^4.10.6",
		"svelte2tsx": "0.5.5",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2",
		"validator": "13.7.0",
		"vite": "^3.2.0",
		"webpack": "5.70.0",
		"xstate": "4.30.6"
	}
}
