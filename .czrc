{
	"path": "packages/config/src/cz-conventional-changelog",
	"name": "cz-conventional-changelog-ffr",
	"punctuation": {
		"type": " ",
		"scope": [
			"[",
			"]"
		]
	},
	"disableSubjectLowerCase": true,
	"types": {
		"🚧 ": {
			"description": "Work in progress",
			"title": "WIP"
		},
		"🌱 ": {
			"description": "Gardening / baby step / cleaning code",
			"title": "Gardening"
		},
		"♻️  ": {
			"description": "Refactor / rename / move",
			"title": "Refactor"
		},
		"🗑️ ": {
			"description": "Deprecate stuff",
			"title": "Deprecate"
		},
		"🔥 ": {
			"description": "Remove dependencies / code / files",
			"title": "Remove"
		},
		"🐛 ": {
			"description": "Fix bug / typo / missing file",
			"title": "Bugfix"
		},
		"💥": {
			"description": "Breaking change",
			"title": "Breaking"
		},
		"➕": {
			"description": "Add dependencies / new files",
			"title": "Add"
		},
		"⬆️ ": {
			"description": "Update dependencies",
			"title": "Update"
		},
		"📐 ": {
			"description": "Format files",
			"title": "Format"
		},
		"🚨": {
			"description": "Fix lint issues",
			"title": "Lint"
		},
		"🧪": {
			"description": "Add / Update tests",
			"title": "Tests"
		},
		"🍱": {
			"description": "Types / TypesScript related changes",
			"title": "Types"
		},
		"❄️ ": {
			"description": "Freeze something",
			"title": "Freeze"
		},
		"🌟": {
			"description": "New feature",
			"title": "Feature"
		},
		"🦋": {
			"description": "Add / Update changeset",
			"title": "Changeset"
		},
		"🎬 ": {
			"description": "Versioning / version tags",
			"title": "Version"
		},
		"🚀": {
			"description": "New release",
			"title": "Release"
		},
		"📝 ": {
			"description": "Add / Update README / doc",
			"title": "Doc"
