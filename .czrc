		"👾": {
			"description": "Graphics stuff",
			"title": "Graphics"
		},
		"🗄️ ": {
			"description": "Database stuff",
			"title": "Database"
		},
		"🐍": {
			"description": "Backend stuff (python)",
			"title": "Backend Python"
		},
		"🛼 ": {
			"description": "Backend stuff (node)",
			"title": "Backend Node"
